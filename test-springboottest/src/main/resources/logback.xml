<?xml version="1.0" encoding="UTF-8"?>
<configuration>
 
 <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
  <encoder>
    <pattern>%d{HH:mm:ss.SSS} %-4relative [%thread] %-5level %logger{35} - %msg %n
   </pattern>
  </encoder>
 </appender>
 
 <root level="INFO">
	<appender-ref ref="CONSOLE" />
 </root>

</configuration>
